{
  "manifest_version": 2,

  "name": "Links manager",
  "description": "Manage useful links",
  "version": "0.1",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/",
    "https://raw.githubusercontent.com/freewind/wind-links-scala-js-react/master/src/main/resources/data/projects.json"
  ]
}
